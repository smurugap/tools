<interface-information style="normal">
{% for n in range(0, count) %}
<physical-interface>
<name>
ge-0/0/{{n}}
</name>
<admin-status format="Enabled">
up
</admin-status>
<oper-status>
up
</oper-status>
<local-index>
{{n}}
</local-index>
<snmp-index>
{{n}}
</snmp-index>
<link-level-type>
Ethernet
</link-level-type>
<speed>
1000mbps
</speed>
<link-mode>
Full-duplex
</link-mode>
<mtu>
1514
</mtu>
<current-physical-address>
9c:cc:83:e4:1:{{ '%02x'|format(n) }}
</current-physical-address>
<hardware-physical-address>
9c:cc:83:e4:1:{{ '%02x'|format(n) }}
</hardware-physical-address>
</physical-interface>
{% endfor %}
<physical-interface>
<name>
lo0
</name>
<admin-status format="Enabled">
up
</admin-status>
<oper-status>
up
</oper-status>
<local-index>
600
</local-index>
<snmp-index>
600
</snmp-index>
<if-type>
Loopback
</if-type>
<mtu>
Unlimited
</mtu>
<logical-interface>
<name>
lo0.0
</name>
<local-index>
601
</local-index>
<snmp-index>
601
</snmp-index>
<address-family>
<address-family-name>
inet
</address-family-name>
<interface-address>
<ifa-flags>
<ifaf-current-default/>
<ifaf-current-primary/>
</ifa-flags>
<ifa-local>
{{ lo_ip }}
</ifa-local>
</interface-address>
</address-family>
</logical-interface>
</physical-interface>
</interface-information>
